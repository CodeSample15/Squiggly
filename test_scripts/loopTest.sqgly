:START: {
    int count = 0

    repeat (5) {
        count += 1
        ^PRINT("Counter: " + count)

        repeat(10) {
            count += 2
            ^PRINT("Inner: " + count)
        }
    }

    count = 0
    while(count < 20) {
        ^PRINT(count)
        count += 1
    }

    ^PRINT("Done")
}

